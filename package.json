{
  "name": "vite-plugin-starter",
  "version": "1.0.0",
  "description": "this is vite plugin template",
  "keywords": [
    "vite-plugin",
    "vite"
  ],
  "license": "MIT",
  "author": "aibayanyu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistjs/vite-plugin-starter.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageMananger": "pnpm@6.32.8",
  "scripts": {
    "prepare": "husky install",
    "test": "vitest",
    "test:run": "vitest run",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "playground": "vite playground"
  },
  "bugs": {
    "url": "https://github.com/mistjs/vite-plugin-starter/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@mistjs/eslint-config": "^0.0.2",
    "@rollup/pluginutils": "^4.2.1",
    "@types/node": "^17.0.24",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "magic-string": "^0.26.1",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue": "^3.2.33"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue}": "eslint . --fix"
  },
  "homepage": "https://github.com/mistjs/vite-plugin-starter#readme"
}
